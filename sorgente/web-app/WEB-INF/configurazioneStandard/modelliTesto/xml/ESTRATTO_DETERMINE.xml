	<queryes>
		<query id="registro"
			help_query_alias="r"
			help_field_aliases="REGISTRO, ANNO_REGISTRO"><![CDATA[
  select  tr.descrizione REGISTRO
       ,  #p_anno ANNO_REGISTRO
    from  tipi_registro tr	   
   where tr.tipo_registro = #p_registro ]]>
</query>	
 	<!-- Determine: restituisce l'elenco delle determine -->
		<query id="estratto_determine"
			help_query_alias="rd"
			help_field_aliases="ANNO_ADOZIONE, ANNO_ALBO, DATA_DETERMINA, DATA_DETERMINA_2, DATA_ESECUTIVITA, DATA_FINE_PUBBLICAZIONE, DATA_PUBBLICAZIONE, OGGETTO, ESTRATTO, NUMERO_ALBO, NUMERO_DETERMINA, NUMERO_DETERMINA_2, STATO, UNITA_PROPONENTE"><![CDATA[
 select decode (stato, 'ANNULLATO', 'ANNULLATA', 'NON_ESECUTIVO', 'ANNULLATA',  '') STATO
       ,  d.numero_determina NUMERO_DETERMINA
       ,  to_char (d.data_numero_determina, 'dd/mm/yyyy') DATA_DETERMINA
       ,  d.numero_determina_2 NUMERO_DETERMINA_2
       ,  to_char (d.data_numero_determina_2, 'dd/mm/yyyy') DATA_DETERMINA_2
       ,  to_char (d.data_esecutivita, 'dd/mm/yyyy') DATA_ESECUTIVITA
       ,  decode (td.pubblicazione, 'Y', to_char (d.data_pubblicazione, 'dd/mm/yyyy'), 'OMESSA') DATA_PUBBLICAZIONE
       ,  to_char (data_fine_pubblicazione, 'dd/mm/yyyy') DATA_FINE_PUBBLICAZIONE
       ,  d.oggetto OGGETTO
       ,  nvl(da.valore, '') ESTRATTO
       ,  utility_pkg.get_unita_prop_determina (d.id_determina) UNITA_PROPONENTE
       ,  d.anno_determina ANNO_ADOZIONE
       ,  d.anno_albo ANNO_ALBO
       ,  d.numero_albo NUMERO_ALBO
    from  determine_soggetti ds_dirigente
       ,  determine_soggetti ds_uo_proponente
       ,  determine d
       ,  tipi_determina td
       ,  dati_aggiuntivi da
   where d.id_tipo_determina = td.id_tipo_determina
     and d.id_determina = ds_dirigente.id_determina(+)
     and ds_dirigente.tipo_soggetto(+) = 'DIRIGENTE'
     and d.id_determina = ds_uo_proponente.id_determina(+)
     and ds_uo_proponente.tipo_soggetto(+) = 'UO_PROPONENTE'
     and d.numero_determina is not null
     and da.id_determina (+) = d.id_determina
     and da.codice (+) = 'ESTRATTO'
     and d.ente = #p_ente and ( (#p_anno is null)
       or (#p_anno is not null
       and d.anno_determina = #p_anno ))
     and ( (#p_da_numero is null)
       or (#p_da_numero is not null
       and d.numero_determina >= #p_da_numero ))
     and ( (#p_a_numero is null)
       or (#p_a_numero is not null
       and d.numero_determina <= #p_a_numero ))
     and ( (#p_uo_proponente is null)
       or (#p_uo_proponente is not null
       and ds_uo_proponente.unita_progr = #p_uo_proponente and ds_uo_proponente.unita_dal = to_date(#p_uo_proponente_dal ,'dd/mm/yyyy')
       and ds_uo_proponente.unita_ottica = #p_uo_proponente_ottica ))
     and ( (#p_codice_dirigente is null)
       or (#p_codice_dirigente is not null
       and ds_dirigente.utente = #p_codice_dirigente ))
     and ( (#p_registro is null)
       or (#p_registro is not null
       and d.registro_determina = #p_registro ))
     and ( (#p_data_pubb_inizio_dal is null)
       or (#p_data_pubb_inizio_dal is not null
       and d.data_pubblicazione >= to_date(#p_data_pubb_inizio_dal ,'dd/mm/yyyy'))
       or (#p_data_pubb_inizio_dal is not null
       and d.data_pubblicazione_2 >= to_date(#p_data_pubb_inizio_dal ,'dd/mm/yyyy')))
     and ( (#p_data_pubb_inizio_al is null)
       or (#p_data_pubb_inizio_al is not null
       and d.data_fine_pubblicazione <= to_date(#p_data_pubb_inizio_al ,'dd/mm/yyyy'))
       or (#p_data_pubb_inizio_al is not null
       and d.data_fine_pubblicazione_2 <= to_date(#p_data_pubb_inizio_al ,'dd/mm/yyyy')))
     and ( (#p_data_esec_dal is null)
       or (#p_data_esec_dal is not null
       and d.data_esecutivita >= to_date(#p_data_esec_dal ,'dd/mm/yyyy')))
     and ( (#p_data_esec_al is null)
       or (#p_data_esec_al is not null
       and d.data_esecutivita <= to_date(#p_data_esec_al ,'dd/mm/yyyy')))
     and ( (#p_data_adozione_dal is null)
       or (#p_data_adozione_dal is not null
       and d.data_numero_determina >= to_date(#p_data_adozione_dal ,'dd/mm/yyyy')))
     and ( (#p_data_adozione_al is null)
       or (#p_data_adozione_al is not null
       and d.data_numero_determina <= to_date(#p_data_adozione_al ,'dd/mm/yyyy')))
union all
select decode (stato, 'ANNULLATO', 'ANNULLATA', 'NON_ESECUTIVO', 'ANNULLATA',  '') STATO
       ,  d.numero_determina NUMERO_DETERMINA
       ,  to_char (d.data_numero_determina, 'dd/mm/yyyy') DATA_DETERMINA
       ,  d.numero_determina_2 NUMERO_DETERMINA_2
       ,  to_char (d.data_numero_determina_2, 'dd/mm/yyyy') DATA_DETERMINA_2
       ,  to_char (d.data_esecutivita, 'dd/mm/yyyy') DATA_ESECUTIVITA
       ,  decode (td.pubblicazione, 'Y', to_char (d.data_pubblicazione, 'dd/mm/yyyy'), 'OMESSA') DATA_PUBBLICAZIONE
       ,  to_char (data_fine_pubblicazione, 'dd/mm/yyyy') DATA_FINE_PUBBLICAZIONE
       ,  d.oggetto OGGETTO
       ,  nvl(da.valore, '') ESTRATTO
       ,  utility_pkg.get_unita_prop_determina (d.id_determina) UNITA_PROPONENTE
       ,  d.anno_determina_2 ANNO_ADOZIONE
       ,  d.anno_albo ANNO_ALBO
       ,  d.numero_albo NUMERO_ALBO
    from  determine_soggetti ds_dirigente
       ,  determine_soggetti ds_uo_proponente
       ,  determine d
       ,  tipi_determina td
       ,  dati_aggiuntivi da
   where d.id_tipo_determina = td.id_tipo_determina
     and d.id_determina = ds_dirigente.id_determina(+)
     and ds_dirigente.tipo_soggetto(+) = 'DIRIGENTE'
     and d.id_determina = ds_uo_proponente.id_determina(+)
     and ds_uo_proponente.tipo_soggetto(+) = 'UO_PROPONENTE'
     and d.numero_determina_2 is not null
     and da.id_determina (+) = d.id_determina
     and da.codice (+) = 'ESTRATTO'
     and d.ente = #p_ente and ( (#p_anno is null)
       or (#p_anno is not null
       and d.anno_determina_2 = #p_anno ))
     and ( (#p_da_numero is null)
       or (#p_da_numero is not null
       and d.numero_determina_2 >= #p_da_numero ))
     and ( (#p_a_numero is null)
       or (#p_a_numero is not null
       and d.numero_determina_2 <= #p_a_numero ))
     and ( (#p_uo_proponente is null)
       or (#p_uo_proponente is not null
       and ds_uo_proponente.unita_progr = #p_uo_proponente and ds_uo_proponente.unita_dal = to_date(#p_uo_proponente_dal ,'dd/mm/yyyy')
       and ds_uo_proponente.unita_ottica = #p_uo_proponente_ottica ))
     and ( (#p_codice_dirigente is null)
       or (#p_codice_dirigente is not null
       and ds_dirigente.utente = #p_codice_dirigente ))
     and ( (#p_registro is null)
       or (#p_registro is not null
       and d.registro_determina_2 = #p_registro ))
     and ( (#p_data_pubb_inizio_dal is null)
       or (#p_data_pubb_inizio_dal is not null
       and d.data_pubblicazione >= to_date(#p_data_pubb_inizio_dal ,'dd/mm/yyyy'))
       or (#p_data_pubb_inizio_dal is not null
       and d.data_pubblicazione_2 >= to_date(#p_data_pubb_inizio_dal ,'dd/mm/yyyy')))
     and ( (#p_data_pubb_inizio_al is null)
       or (#p_data_pubb_inizio_al is not null
       and d.data_fine_pubblicazione <= to_date(#p_data_pubb_inizio_al ,'dd/mm/yyyy'))
       or (#p_data_pubb_inizio_al is not null
       and d.data_fine_pubblicazione_2 <= to_date(#p_data_pubb_inizio_al ,'dd/mm/yyyy')))
     and ( (#p_data_esec_dal is null)
       or (#p_data_esec_dal is not null
       and d.data_esecutivita >= to_date(#p_data_esec_dal ,'dd/mm/yyyy')))
     and ( (#p_data_esec_al is null)
       or (#p_data_esec_al is not null
       and d.data_esecutivita <= to_date(#p_data_esec_al ,'dd/mm/yyyy')))
     and ( (#p_data_adozione_dal is null)
       or (#p_data_adozione_dal is not null
       and d.data_numero_determina_2 >= to_date(#p_data_adozione_dal ,'dd/mm/yyyy')))
     and ( (#p_data_adozione_al is null)
       or (#p_data_adozione_al is not null
       and d.data_numero_determina_2 <= to_date(#p_data_adozione_al ,'dd/mm/yyyy')))
 order by ANNO_ADOZIONE, NUMERO_DETERMINA_2, NUMERO_DETERMINA ]]>
</query>
	</queryes>
